  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "sourceMapsAbsolute": false,
          "sourceMaps": true,
          "plugins": [
            [
              "module:fast-async",
              {
                "runtimePattern": null,
                "compiler": {
                  "es7": true,
                  "noRuntime": true,
                  "promises": true,
                  "wrapAwait": true
                }
              }
            ],
            [
              "module:babel-plugin-yo-yoify"
            ],
            [
              "module:@babel/plugin-transform-object-assign"
            ]
          ],
          "presets": [
            "@babel/preset-env"
          ]
        }
      ]
    ]
  },
